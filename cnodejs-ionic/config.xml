<?xml version='1.0' encoding='utf-8'?>
<widget id="com.lanceli.cnodejs"
  version="1.2.1"
  android-versionCode="103"
  ios-CFBundleVersion="103"
  xmlns="http://www.w3.org/ns/widgets"
  xmlns:cdv="http://cordova.apache.org/ns/1.0">
